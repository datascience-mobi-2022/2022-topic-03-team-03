# added if so we get 0 instead of Na if RowSum is 0
rowPercent <- function(dataframe){
dt <- dataframe
lastrow <- dim(dataframe)[1]
for(i in 1:lastrow){
RowSum <- rowSums (dataframe[i,])
if (RowSum == 0) {dt[i,] <- 0}
else dt[i,] <- (dataframe[i,]/RowSum)*100    }
return(dt)}
