#Spalten normalisieren
Spalten_Norm <- function(x){
ColSumme = colSums(x)
maxcolsums = c()
k = 3
for (i in 1:(ncol(x)/3)) {
a = ColSumme[[k*i]]
b = ColSumme[[k*i-1]]
c = ColSumme[[k*i-2]]
maxcolsums = c(maxcolsums, max(a,b,c))
}
return(ColSumme)
}
Spalten_Norm_1 <- function(x){
ColSumme = colSums(x)
maxcolsums = c()
k = 3
for (i in 1:(ncol(x)/3)) {
a = ColSumme[[k*i]]
b = ColSumme[[k*i-1]]
c = ColSumme[[k*i-2]]
maxcolsums = c(maxcolsums, max(a,b,c))
}
return(maxcolsums)
}
Spalten_Norm_2 <- function(x, ColSumme, maxcolsums){
Quotient = c()
k = 3
for (i in 1:(ncol(x)/3)){
Quotient = c(Quotient, maxcolsums[i]/ColSumme[[k*i-2]])
Quotient = c(Quotient, maxcolsums[i]/ColSumme[[k*i-1]])
Quotient = c(Quotient, maxcolsums[i]/ColSumme[[k*i]])
}
return(Quotient)
}
